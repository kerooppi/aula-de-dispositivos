package com.example.navegao;

import androidx.appcompat.app.AppCompatActivity;

import android.content.Intent;
import android.os.Bundle;
import android.view.View;

public class Telainicial extends AppCompatActivity {

    @Override
    protected void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
        setContentView(R.layout.activity_telainicial);
    }

    public void Tela1(View view){
        Intent intent = new Intent(Telainicial.this, Tela1.class);
        startActivity(intent);

    }

    public void Tela2(View view){
        Intent intent = new Intent(Telainicial.this, Tela2.class);
        startActivity(intent);

    }

    public void Tela3(View view) {
        Intent intent = new Intent(Telainicial.this, Tela3.class);
        startActivity(intent);

    }

    public void Tela4(View view) {
        Intent intent = new Intent(Telainicial.this, Tela4.class);
        startActivity(intent);

    }
}
